{
  "name": "tarkov.dev",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "npm run prebuild",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,ts,tsx,json,css,scss,md}\"",
    "eject": "react-scripts eject",
    "prebuild": "node scripts/update-props.mjs && node scripts/cache-api-data.mjs && node scripts/get-version.mjs && node scripts/get-contributors.mjs && node scripts/build-sitemap.mjs && node scripts/generate-thumbnails.mjs",
    "postbuild": "node scripts/build-redirects.mjs && node scripts/critical.mjs",
    "stage": "npx rimraf build && npm run build && npm run preview",
    "preview": "npx serve build -l 3001 -s",
    "critical": "node scripts/critical.mjs",
    "cache-api-data": "node scripts/cache-api-data.mjs"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/material": "^7.2.0",
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "classnames": "^2.5.1",
    "critical": "^7.2.0",
    "dayjs": "^1.11.18",
    "fast-deep-equal": "^3.1.3",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "lz-string": "^1.5.0",
    "react": "^18.3.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.10.2",
    "react-spinners": "^0.17.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-explorer": "^2.5.3"
  },
  "jest": {
    "transformIgnorePatterns": []
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4
  }
}
